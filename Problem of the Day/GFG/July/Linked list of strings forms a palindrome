/*
08-07-24
Linked list of strings forms a palindrome
*/
class Solution {
    public boolean compute(Node head) {
        String s="";
        while(head!=null)
        {
            s=s+head.data;
            head=head.next;
        }

        int start = 0, end = s.length() - 1;
        while (start <= end) {
            if (s.charAt(start++) != s.charAt(end--)) {
                return false;
            }
        }
        return true;
        
    }}
